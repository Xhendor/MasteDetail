


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools" android:id="@+id/participante_detail_container"
android:layout_width="match_parent" android:layout_height="match_parent"
tools:context=".ParticipanteDetailActivity" tools:ignore="MergeRootFrame">
    <TextView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" android:id="@+id/participante_detail"
        style="?android:attr/textAppearanceLarge" android:layout_width="match_parent"
        android:layout_height="wrap_content" android:padding="16dp" android:textIsSelectable="true"
        tools:context=".ParticipanteDetailFragment"
        android:textColor="#ff000000"
        android:textSize="35dp" />

    <TextView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" android:id="@+id/participante_detailA"
        style="?android:attr/textAppearanceLarge" android:layout_width="match_parent"
        android:layout_height="wrap_content" android:padding="16dp" android:textIsSelectable="true"
        tools:context=".ParticipanteDetailFragment"
        android:layout_below="@+id/participante_detail"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
